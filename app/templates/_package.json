{
  "name": "<%= repositoryName %>",
  "version": "0.1.0",
  "description": "<%= appDescription %>",
  "license": "UNLICENSED",
  "private": true,
  "author": {
    "name": "<%= appAuthorName %>",
    "email": "<%= appAuthorEmail %>"
  },
  "repository": {
    "type": "git",
    "url": "https://dev.azure.com/dmwm/<%= repositoryName %>/_git/<%= repositoryName %>"
  },
  "scripts": {
    "development": "blendid -- default",
    "production": "blendid -- build",
    "dist-patch": "blendid -- dist-update && blendid -- tag:patch",
    "dist-update": "blendid -- dist-update && blendid -- tag:minor",
    "dist-upgrade": "blendid -- dist-update && blendid -- tag:major"
  },
  "devDependencies": {
     "queo-blendid": "^4.1.18",
    ,"npm-sass-require": "^0.1.0",
    ,"stylelint-config-recommended": "^2.2.0"
    <% if( cssPreprocessor === 'sass' && enableStyleguide === true ) { %>
    ,"queo-styleguide": "^1.1.0"
    <% } %>
    <% if( enableTypescript === true ) { %>
    ,"ts-loader": "^2.3.7"
    ,"typescript": "^2.4.2"
    <% } %>
  },
  "dependencies": {
    "jquery": "^3.4.0"
    <% if( bootstrapName !== null ) { %>,"<%= bootstrapName %>": "<%= bootstrapVersion %>"<% } %>
    <% if( cssPreprocessor == 'sass' && enableStyleguide === true ) { %>,"highlightjs": "^9.12.0"<% } %>
  }
}
